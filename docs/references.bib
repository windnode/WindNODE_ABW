@article{Wiese2019,
title = {Open Power System Data – Frictionless data for electricity system modelling},
journal = {Applied Energy},
volume = {236},
pages = {401-409},
year = {2019},
issn = {0306-2619},
doi = {https://doi.org/10.1016/j.apenergy.2018.11.097},
url = {https://www.sciencedirect.com/science/article/pii/S0306261918318130},
author = {Frauke Wiese and Ingmar Schlecht and Wolf-Dieter Bunke and Clemens Gerbaulet and Lion Hirth and Martin Jahn and Friedrich Kunz and Casimir Lorenz and Jonathan Mühlenpfordt and Juliane Reimann and Wolf-Peter Schill},
keywords = {Electricity system modelling, Frictionless data, Data transparency, Data package, Data platform, Open data},
abstract = {The quality of electricity system modelling heavily depends on the input data used. Although a lot of data is publicly available, it is often dispersed, tedious to process and partly contains errors. We argue that a central provision of input data for modelling has the character of a public good: it reduces overall societal costs for quantitative energy research as redundant work is avoided, and it improves transparency and reproducibility in electricity system modelling. This paper describes the Open Power System Data platform that aims at realising the efficiency and quality gains of centralised data provision by collecting, checking, processing, aggregating, documenting and publishing data required by most modellers. We conclude that the platform can provide substantial benefits to energy system analysis by raising efficiency of data pre-processing, providing a method for making data pre-processing for energy system modelling traceable, flexible and reproducible and improving the quality of original data published by data providers.}
}

